{
    "name": "khodja/upload",
    "description": "File upload and thumbnail management package for laravel 5.*",
    "type": "library",
    "require": {
        "php": ">=5.4.0",
        "intervention/image": "~2.3"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Fayzulla Khodja",
            "email": "fayzulla@khodja.uz"
        }
    ],
    "autoload": {
        "psr-4": {
            "Khodja\\Upload\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Intervention\\Image\\ImageServiceProvider",
                "Khodja\\Upload\\UploadServiceProvider"
            ],
            "aliases": {
                "Upload": "Khodja\\Upload\\Facades\\Upload"
            }
        }
    }
}
